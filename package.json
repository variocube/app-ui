{
  "name": "@variocube/app-ui",
  "version": "0.0.5",
  "description": "Common UI components for Variocube applications.",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "dev": "webpack serve --progress",
    "clean": "rimraf esm/",
    "copyfiles": "copyfiles -u 1 src/**/*.svg esm/",
