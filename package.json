  },
  "files": [
    "esm/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.5",
    "copyfiles": "^2.4.1",
    "file-loader": "^6.2.0",
    "rimraf": "^2.7.1",
    "source-map-loader": "^2.0.2",
    "ts-loader": "^9.2.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
